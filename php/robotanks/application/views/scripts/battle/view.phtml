<script type="text/javascript">//------------------------------------------//XML Parsing function starts now//-------------------------------------------function parseXML(ROBO) {	var i=0; //used for storage array size designator	var arRob=new array; //array for storage of robot parametres	$.get(ROBO,{},function(xml) {		$('robots',xml).each(function() {			i++;			//var now=new array;			arRob[i]=$(this).find("vsName").text();		});	});	}//--------------------------------------------</script>